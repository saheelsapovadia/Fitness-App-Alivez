<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bottomnav">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectWorkout"/>
        <activity android:name=".selectManageClients"/>
        <activity android:name=".SelectBMI"/>
        <activity android:name=".SelectDiet"/>
        <activity android:name=".IntroActivity" />
        <activity android:name=".login"/>
        <activity android:name=".loginStartup" />
        <activity
            android:name=".ClientBottomNav"
            android:label="@string/title_activity_client_bottom_nav"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name=".TrainerBottomNav"
            android:label="@string/title_activity_trainer_bottom_nav"
            android:windowSoftInputMode="adjustPan">
        </activity>
        <activity
            android:name=".SplashScreen"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>